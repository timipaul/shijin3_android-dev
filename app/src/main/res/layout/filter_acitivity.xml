<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <include layout="@layout/common_title" />

    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:overScrollMode="never">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_marginTop="@dimen/dp_15"
                android:text="@string/sex"
                android:textColor="@color/text_33"
                android:textSize="@dimen/sp_15" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_10"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_male"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/dp_30"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:layout_weight="1"
                    android:background="@drawable/btn_unchecked"
                    android:gravity="center"
                    android:text="@string/male"
                    android:textColor="@color/text_33"
                    android:textSize="@dimen/sp_14" />

                <TextView
                    android:id="@+id/tv_female"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/dp_30"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:layout_weight="1"
                    android:background="@drawable/btn_unchecked"
                    android:gravity="center"
                    android:text="@string/female"
                    android:textColor="@color/text_33"
                    android:textSize="@dimen/sp_14" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:layout_marginRight="@dimen/dp_10"
                    android:layout_weight="2" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_marginRight="@dimen/dp_10"
                android:layout_marginTop="@dimen/dp_15"
                android:background="@color/mine_function_divider" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_marginTop="@dimen/dp_15"
                android:text="@string/age"
                android:textColor="@color/text_33"
                android:textSize="@dimen/sp_15" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_10"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_70s"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/dp_30"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:layout_weight="1"
                    android:background="@drawable/btn_unchecked"
                    android:gravity="center"
                    android:text="@string/after70"
                    android:textColor="@color/text_33"
                    android:textSize="@dimen/sp_14" />

                <TextView
                    android:id="@+id/tv_80s"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/dp_30"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:layout_weight="1"
                    android:background="@drawable/btn_unchecked"
                    android:gravity="center"
                    android:text="@string/after80"
                    android:textColor="@color/text_33"
                    android:textSize="@dimen/sp_14" />

                <TextView
                    android:id="@+id/tv_90s"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/dp_30"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:layout_weight="1"
                    android:background="@drawable/btn_unchecked"
                    android:gravity="center"
                    android:text="@string/after90"
                    android:textColor="@color/text_33"
                    android:textSize="@dimen/sp_14" />

                <TextView
                    android:id="@+id/tv_00s"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/dp_30"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:layout_marginRight="@dimen/dp_10"
                    android:layout_weight="1"
                    android:background="@drawable/btn_unchecked"
                    android:gravity="center"
                    android:text="@string/after00"
                    android:textColor="@color/text_33"
                    android:textSize="@dimen/sp_14" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_marginRight="@dimen/dp_10"
                android:layout_marginTop="@dimen/dp_15"
                android:background="@color/mine_function_divider" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_10"
                android:layout_marginTop="@dimen/dp_15"
                android:text="@string/job"
                android:textColor="@color/text_33"
                android:textSize="@dimen/sp_15" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_10"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_doctor"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/dp_30"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:layout_weight="1"
                    android:background="@drawable/btn_unchecked"
                    android:gravity="center"
                    android:text="@string/doctor"
                    android:textColor="@color/text_33"
                    android:textSize="@dimen/sp_14" />

                <TextView
                    android:id="@+id/tv_mom"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/dp_30"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:layout_weight="1"
                    android:background="@drawable/btn_unchecked"
                    android:gravity="center"
                    android:text="@string/mom"
                    android:textColor="@color/text_33"
                    android:textSize="@dimen/sp_14" />

                <TextView
                    android:id="@+id/tv_white"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/dp_30"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:layout_weight="1"
                    android:background="@drawable/btn_unchecked"
                    android:gravity="center"
                    android:text="@string/white_work"
                    android:textColor="@color/text_33"
                    android:textSize="@dimen/sp_14" />

                <TextView
                    android:id="@+id/tv_it"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/dp_30"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:layout_marginRight="@dimen/dp_10"
                    android:layout_weight="1"
                    android:background="@drawable/btn_unchecked"
                    android:gravity="center"
                    android:text="@string/IT"
                    android:textColor="@color/text_33"
                    android:textSize="@dimen/sp_14" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_10"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/tv_free"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="@dimen/dp_30"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:textColor="@color/text_33"
                    android:gravity="center"
                    android:textSize="@dimen/sp_14"
                    android:text="@string/free_work"
                    android:background="@drawable/btn_unchecked"
                    />
                <TextView
                    android:id="@+id/tv_teacher"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="@dimen/dp_30"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:textColor="@color/text_33"
                    android:gravity="center"
                    android:textSize="@dimen/sp_14"
                    android:text="@string/teacher"
                    android:background="@drawable/btn_unchecked"
                    />
                <TextView
                    android:id="@+id/tv_other"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="@dimen/dp_30"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:textColor="@color/text_33"
                    android:gravity="center"
                    android:textSize="@dimen/sp_14"
                    android:text="@string/other"
                    android:background="@drawable/btn_unchecked"
                    />
                <View
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="@dimen/dp_30"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:layout_marginRight="@dimen/dp_10"
                    />
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_5"
                android:background="@color/mine_function_divider"
                android:layout_marginTop="@dimen/dp_15"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_50"
                android:orientation="horizontal"
                android:gravity="center_vertical">
                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_10"
                    android:drawableLeft="@mipmap/icon_positioning"
                    android:text="@string/locate"
                    android:textSize="@dimen/sp_15"
                    android:textColor="@color/text_33"
                    android:drawablePadding="@dimen/dp_5"/>
                <TextView
                    android:id="@+id/tv_loca"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/text_999999"
                    android:textSize="@dimen/sp_14"
                    android:text="@string/country"
                    android:layout_marginRight="@dimen/dp_10"/>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:background="@color/mine_function_divider"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_40"
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/dp_10"
                android:gravity="center_vertical">
                <TextView
                    android:id="@+id/tv_choice_city"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:layout_height="match_parent"
                    android:text="深圳"
                    android:gravity="center"
                    android:paddingLeft="@dimen/dp_10"
                    android:textSize="@dimen/sp_14"
                    android:textColor="@color/text_33"
                    android:drawableRight="@mipmap/icon_dropdown"
                    />
                <View
                    android:layout_width="@dimen/dp_1"
                    android:layout_height="@dimen/dp_20"
                    android:layout_marginLeft="@dimen/dp_15"
                    android:background="@color/mine_function_divider"/>
                <TextView
                    android:id="@+id/tv_loca_here"
                    android:layout_width="@dimen/dp_135"
                    android:layout_height="@dimen/dp_40"
                    android:gravity="center"
                    android:background="@drawable/bg_locate"
                    android:layout_marginLeft="@dimen/dp_15"
                    android:padding="@dimen/dp_10"
                    android:drawablePadding="@dimen/dp_5"
                    android:textColor="@color/text_33"
                    android:textSize="@dimen/sp_14"
                    android:text="@string/location_here"
                    android:drawableLeft="@mipmap/icon_icon_positioning_2"/>

            </LinearLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/put_range"
                android:textSize="@dimen/sp_15"
                android:textColor="@color/text_33"
                android:layout_marginTop="@dimen/dp_15"
                android:layout_marginLeft="@dimen/dp_10"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_40"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:layout_marginLeft="@dimen/dp_10">
                <ImageView
                    android:id="@+id/img_range"
                    android:layout_width="@dimen/dp_12"
                    android:layout_height="@dimen/dp_12"
                    android:src="@mipmap/icon_selected_3"/>
                <TextView
                    android:id="@+id/tv_range"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/dp_5"
                    android:text="@string/northwest"
                    android:textColor="@color/text_999999"
                    android:textSize="@dimen/sp_14"
                    android:gravity="center"
                    android:drawableRight="@mipmap/icon_dropdown_3"
                    android:drawablePadding="@dimen/dp_4"/>
                <ImageView
                    android:id="@+id/img_all_city"
                    android:layout_width="@dimen/dp_12"
                    android:layout_height="@dimen/dp_12"
                    android:layout_marginLeft="@dimen/dp_25"
                    android:src="@mipmap/icon_selected_3"/>
                <TextView
                    android:id="@+id/tv_all_city"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/dp_5"
                    android:text="@string/all_city"
                    android:textColor="@color/text_999999"
                    android:textSize="@dimen/sp_14"
                    android:gravity="center"
                    />
                <ImageView
                    android:id="@+id/img_all_area"
                    android:layout_width="@dimen/dp_12"
                    android:layout_height="@dimen/dp_12"
                    android:layout_marginLeft="@dimen/dp_25"
                    android:src="@mipmap/icon_selected_3"/>
                <TextView
                    android:id="@+id/tv_all_area"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/dp_5"
                    android:text="@string/all_area"
                    android:textColor="@color/text_999999"
                    android:textSize="@dimen/sp_14"
                    android:gravity="center"
                    />
                <ImageView
                    android:id="@+id/img_kilo_range"
                    android:layout_width="@dimen/dp_12"
                    android:layout_height="@dimen/dp_12"
                    android:layout_marginLeft="@dimen/dp_25"
                    android:src="@mipmap/icon_selected_3"/>
                <TextView
                    android:id="@+id/tv_kilo_range"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="@dimen/dp_5"
                    android:text="@string/kilo_1"
                    android:drawableRight="@mipmap/icon_dropdown_3"
                    android:drawablePadding="@dimen/dp_4"
                    android:textColor="@color/text_999999"
                    android:textSize="@dimen/sp_14"
                    android:gravity="center"
                    />
            </LinearLayout>
            <com.baidu.mapapi.map.TextureMapView
                android:id="@+id/map"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_400"/>
        </LinearLayout>
    </ScrollView>

</LinearLayout>
